# Environment variables for docker-compose.yml

# GitHub repository name (format: username/repository)
# Example: GITHUB_REPOSITORY=dodus/carnaby.sk
GITHUB_REPOSITORY=your-username/carnaby.sk

# Optional: Watchtower notification URL (for Slack, Discord, etc.)
# Example for Slack: WATCHTOWER_NOTIFICATION_URL=slack://token@channel
# Leave empty if you don't want notifications
WATCHTOWER_NOTIFICATION_URL=
